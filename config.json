{
  "app": {
    "name": "AgentAI",
    "version": "1.0.0",
    "bundle_id": "com.agentai.app",
    "deployment_target": "16.0"
  },
  "models": {
    "primary": "gpt-4",
    "fallback": "gpt-3.5-turbo",
    "use_ensemble": true,
    "max_tokens": 2048,
    "temperature": 0.7,
    "local_models": [
      "SentimentModel_v1.0",
      "IntentModel_v1.0",
      "LanguageModel_v1.0"
    ]
  },
  "integrations": {
    "enabled": [
      "Calendar",
      "Reminders",
      "Contacts",
      "Weather",
      "News"
    ],
    "optional": [
      "Messages",
      "Mail",
      "Notes"
    ]
  },
  "nlp": {
    "supported_languages": ["en", "es", "fr", "de", "zh"],
    "default_language": "en",
    "sentiment_analysis": true,
    "entity_extraction": true,
    "intent_classification": true
  },
  "personalization": {
    "learn_from_interactions": true,
    "adapt_communication_style": true,
    "remember_preferences": true,
    "context_window_size": 10
  },
  "workflows": {
    "max_steps": 10,
    "timeout_seconds": 30,
    "retry_attempts": 3,
    "parallel_execution": false
  },
  "features": {
    "voice_input": false,
    "voice_output": false,
    "proactive_suggestions": false,
    "offline_mode": false,
    "widget_support": false
  },
  "privacy": {
    "store_locally": true,
    "encrypt_data": true,
    "share_anonymized_analytics": false,
    "request_permissions_explicitly": true
  },
  "ui": {
    "theme": "system",
    "animation_enabled": true,
    "haptic_feedback": true,
    "accessibility_enabled": true
  }
}
